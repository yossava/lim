<p id="notice"><%= notice %></p>

<h1>Listing Financelogs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Toko</th>
      <th>Jumlah</th>
      <th>Produk</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @financelogs.each do |financelog| %>
      <tr>
        <td><%= financelog.user_id %></td>
        <td><%= financelog.toko_id %></td>
        <td><%= financelog.jumlah %></td>
        <td><%= financelog.produk_id %></td>
        <td><%= link_to 'Show', financelog %></td>
        <td><%= link_to 'Edit', edit_financelog_path(financelog) %></td>
        <td><%= link_to 'Destroy', financelog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Financelog', new_financelog_path %>
