											<div class="hargaasli"><%= number_to_currency(produkfashion.harga_diskon, locale: :id) %></div>
                      <div class="hargacoret"><%= number_to_currency(produkfashion.harga, locale: :id) %></div>
                      <% else %>
                      <div class="hargaasli"><%= number_to_currency(produkfashion.harga, locale: :id) %></div>
                      <div class="hargacoret">&nbsp;</div>
                      <% end %>
											<%= link_to Toko.find(produkfashion.toko_id), :class => "tambahketroli", "data-no-turbolink" => true do %><span class="troliblack"></span><span class="ttb"><%= Toko.find(produkfashion.toko_id).nama_toko %></span><% end %>
										</div>
