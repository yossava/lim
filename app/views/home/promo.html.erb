								<nav class="headingtab">
									<h4>Fashion Promo</h4>
								</nav>
								<div class="content isitab normal">
									<section>
                    <% Produk.where(:id => [Homeitem.find(19).text1.split(",")]).each do |produkfashion| %>
                    <% if produkfashion %>
										<div class="produkcarousel normalproduk">
											<div class="thumbfoto-produk">
												<%= link_to produkfashion, "data-no-turbolink" => true do %><img src="<%= produkfashion.foto_produk1.url %>" class="responsiveimg"/><% end %>
                        <% if produkfashion.harga_diskon %>
												<span class="lagidiskon">-<%= produkfashion.diskon %>%</span>
                        <% end %>
                        <% if produkfashion.jumlah_grosir1min %>
                          <span class="grosiran">grosir</span>
                          <% end %>
											</div>
											<%= link_to produkfashion, "data-no-turbolink" => true do %><h4 class="titleproduk"><%= produkfashion.nama_produk %></h4><% end %>
